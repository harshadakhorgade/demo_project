[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/deploy_project/demo_project
ExecStart=/home/ubuntu/deploy_project/demo_project/venv/bin/gunicorn \
          --access-logfile - \
          --workers 3 \
          --bind unix:/run/gunicorn.sock \
         demo_project.wsgi:application
[Install]
WantedBy=multi-user.target
